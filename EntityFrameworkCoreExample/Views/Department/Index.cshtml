
@model List<Department>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}


    <div class="container">
        <div class="row">
            <table class="table">
                <thead>
                    <th>DepartmentId</th>
                    <th>DepartmentName</th>
                    <th>LocationName</th>
                </thead>
                <tbody>
                    @foreach(var dept in Model)
                    {
                    <tr>
                        <th>@dept.Id</th>
                        <th>@dept.Name</th>
                        <th>@dept.Location.Name</th>
                    </tr>
                    }
                </tbody>
            </table>

        </div>
    </div>
